 import javax.swing.*;
public class Marks
{
  public static void main(String args[])
  {
    int marks;
    String smarks=JOptionPane.showInputDialog(null,"enter the student marks");
    marks=Integer.parseInt(smarks);
    if(marks==100)
    JOptionPane.showMessageDialog(null,"perfect score");
     else if(marks>=90&&marks<100)
    JOptionPane.showMessageDialog(null,"excellent");
      else if(marks>=80&&marks<90)
    JOptionPane.showMessageDialog(null,"good");
     else if(marks>=70&&marks<80)
    JOptionPane.showMessageDialog(null,"above average");
     else if(marks>=60&&marks<70)
    JOptionPane.showMessageDialog(null,"average");
     else if(marks>=50&&marks<60)
    JOptionPane.showMessageDialog(null,"below average");
     else if(marks>=0&&marks<50)
    JOptionPane.showMessageDialog(null,"not passed");
  }
}
